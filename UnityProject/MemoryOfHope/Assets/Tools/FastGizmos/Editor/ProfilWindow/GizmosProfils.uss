.ListProfils {
    margin-left: 0;
    margin-right: 0;
    margin-top: 7px;
    margin-bottom: 7px;
    font-size: 14px;
    -unity-font: url('/Assets/TextMesh Pro/Examples & Extras/Fonts/Oswald-Bold.ttf');
    border-left-color: rgba(255, 255, 255, 255);
    border-right-color: rgba(255, 255, 255, 255);
    border-top-color: rgba(255, 255, 255, 255);
    border-bottom-color: rgba(255, 255, 255, 255);
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    white-space: nowrap;
    -unity-text-align: middle-center;
    justify-content: center;
}
